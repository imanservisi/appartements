{% block body %}
<div class="m-3 col-6 card">
    <div class="card-header">
        <h1 class="text-center">Mandat Gestionnaire</h1>
    </div>
    <div class="card-body">
        <table class="table card-table">
            <thead>
                <tr>
                    <th>Début Mandat</th>
                    <th>Fin Mandat</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
            {# {{ dump(mandat_gestionnaires) }} #}
            {% for mandat_gestionnaire in mandat_gestionnaires %}
                <tr>
                    <td>{{ mandat_gestionnaire.debutMandat ? mandat_gestionnaire.debutMandat|date('Y-m-d') : '' }}</td>
                    <td>{{ mandat_gestionnaire.finMandat ? mandat_gestionnaire.finMandat|date('Y-m-d') : '' }}</td>
                    <td>
                        {# <button class="btn btn-info"><a href="{{ path('app_prime_assurance_edit', {residenceId: residence.id, lotId: lot.id, id: prime_assurance.id })}}">Modifier</a></button> #}
                        {# {{ include('prime_assurance/_delete_form.html.twig', {residenceId: residence.id, lotId: lot.id}) }} #}
                    </td>
                </tr>
            {% else %}
                <tr>
                    <td colspan="5">aucune donnée</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
        {# <div class="text-center">
            <button class="btn btn-primary"><a href="{{ path('app_prime_assurance_new', {residenceId: residence.id, lotId: lot.id}) }}">Ajouter une prime d'assurance</a></button>
        </div> #}
    </div>
</div>
{% endblock %}
